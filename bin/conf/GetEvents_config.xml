<?xml version="1.0"?>
<getthis reportall="">
    <location>%SystemDrive%</location>
    <samples MaxPerSampleBytes="10GB" MaxTotalBytes="25GB" MaxSampleCount="200000">
        <sample name="evtx">
            <ntfs_find name_match="*.evtx" header="ElfFile"/>
        </sample>
        <sample name="evt">
            <ntfs_find name_match="*.evt" header_hex="300000004c664c65"/>
        </sample>
    </samples>
</getthis>